{{outlet}}

<h1>Invitations</h1>

<table class="table table-bordered table-striped">
  <thread class="col-sm-10">
    <tr>
      <th >ID</th>
      <th >Email</th>
    </tr>
  </thread>
  <tbody>
  {{#each model as |invitation|}}
    <tr>
      <td >{{invitation.id}}</td>
      <td >{{invitation.email}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>